{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "outDir": "../"
  },
  "compileOnSave": true,
  "include": [
    "*.*"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
